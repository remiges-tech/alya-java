{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngxAlyavalidation": {
      "projectType": "library",
      "root": "projects/ngx-alyavalidation",
      "sourceRoot": "projects/ngx-alyavalidation/src",
      "prefix": "lib",
      "i18n": {
        "sourceLocale":"en-US",
        "locales": {
          
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
"project": "projects/ngx-alyavalidation/ng-package.json"

          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-alyavalidation/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-alyavalidation/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-alyavalidation/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
